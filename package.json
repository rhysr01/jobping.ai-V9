{
	"name": "jobping",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "NODE_OPTIONS='--no-deprecation' next build --webpack",
		"start": "next start",
		"lint": "next lint",
		"lint:biome": "biome check .",
		"lint:biome:fix": "biome check --write .",
		"format": "biome format --write .",
		"type-check": "tsc --noEmit",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:free": "playwright test tests/e2e/free-version-loading.spec.ts",
		"test:all": "npm run test && npm run test:e2e && npm run test:e2e:performance && npm run test:security && npm run test:database-integrity && npm run test:e2e:user-scenarios",
		"test:e2e:complete": "playwright test tests/e2e/complete-free-signup.spec.ts tests/e2e/complete-premium-signup.spec.ts",
		"test:e2e:api-validation": "playwright test tests/e2e/api-endpoint-validation.spec.ts",
		"test:e2e:performance": "playwright test tests/e2e/performance-validation.spec.ts",
		"test:e2e:user-scenarios": "playwright test tests/e2e/real-user-scenarios.spec.ts tests/e2e/premium-user-scenarios.spec.ts",
		"test:analysis": "playwright test tests/e2e/real-user-scenarios.spec.ts --grep \"COMPREHENSIVE MATCHING ALGORITHM ANALYSIS\"",
		"test:security": "playwright test tests/security/input-validation.spec.ts",
		"test:database-integrity": "playwright test tests/integration/database-integrity.spec.ts",
		"test:ai-comprehensive": "tsx scripts/test-ai-comprehensive.ts",
		"test:production-engine": "tsx scripts/test-production-matching-engine.ts",
		"test:ai-check": "./check-env-and-test.sh",
		"monitor:ai-production": "tsx scripts/monitor-ai-production.ts",
		"test:free": "tsx scripts/test-free-version.ts",
		"debug:free": "tsx scripts/debug-free-signup.ts",
		"test:signup": "tsx scripts/test-free-signup-flow.ts",
		"check:jobs": "tsx scripts/check-jobs-database.ts",
		"automation:start": "node automation/real-job-runner.cjs",
		"cleanup:jobs": "tsx scripts/cleanup-jobs.ts",
		"health:check": "curl -f http://localhost:3000/api/health || exit 1",
		"pilot:smoke": "tsx scripts/pilot-smoke.ts",
		"limiter:test": "tsx scripts/lock-rate-limit.ts",
		"automation:embeddings": "node automation/embedding-refresh.cjs",
		"check:performance": "tsx scripts/check-performance.ts",
		"analyze-bundle": "ANALYZE=true npm run build",
		"generate-secrets": "tsx scripts/generate-secrets.ts",
		"verify:env": "tsx scripts/verify-env-services.ts",
		"mcp:start": "tsx scripts/start-mcp-server.ts",
		"mcp:dev": "tsx scripts/start-mcp-server.ts",
		"clean": "rm -rf .next out coverage test-results",
		"pre-push": "npm run build",
		"postinstall": "next telemetry disable"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.25.2",
		"@octokit/rest": "22.0.1",
		"@polar-sh/nextjs": "0.9.3",
		"@sentry/nextjs": "10.34.0",
		"@sentry/node": "10.34.0",
		"@supabase/supabase-js": "2.90.1",
		"@tsparticles/engine": "3.9.1",
		"@tsparticles/react": "3.0.0",
		"@tsparticles/slim": "3.9.1",
		"@types/canvas-confetti": "1.9.0",
		"@vercel/og": "0.8.6",
		"axios": "1.13.2",
		"canvas-confetti": "1.9.4",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"csv-parse": "6.1.0",
		"dayjs": "1.11.19",
		"dotenv": "17.2.3",
		"framer-motion": "12.26.2",
		"inngest": "3.49.1",
		"lru-cache": "11.2.4",
		"lucide-react": "0.562.0",
		"next": "16.1.2",
		"next-axiom": "1.10.0",
		"node-cron": "3.0.3",
		"openai": "^4.20.1",
		"puppeteer": "24.35.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-hot-toast": "2.6.0",
		"redis": "4.7.1",
		"resend": "^6.7.0",
		"stripe": "17.7.0",
		"tailwind-merge": "3.4.0",
		"web-vitals": "3.5.2",
		"zod": "4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@jest/globals": "30.2.0",
		"@next/bundle-analyzer": "16.1.2",
		"@playwright/test": "^1.40.1",
		"@testing-library/dom": "10.4.1",
		"@testing-library/jest-dom": "6.9.1",
		"@testing-library/react": "16.3.1",
		"@testing-library/user-event": "14.6.1",
		"@types/jest": "^29.5.8",
		"@types/node": "^24.0.0",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"autoprefixer": "10.4.23",
		"eslint": "^8.54.0",
		"eslint-config-next": "16.1.2",
		"fast-check": "4.5.3",
		"glob": "13.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "30.2.0",
		"knip": "5.81.0",
		"node-mocks-http": "1.17.2",
		"pg": "8.17.1",
		"playwright": "^1.40.1",
		"postcss": "8.5.6",
		"tailwindcss": "^3.3.5",
		"tsx": "^4.21.0",
		"typescript": "5.9.3",
		"wait-on": "^7.0.1"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	}
}
